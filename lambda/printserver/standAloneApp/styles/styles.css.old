/* Slide Panel */
.slide-panes {
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  position: static;
}
.slide-panes > .slide-handler {
  background: transparent;
  position: absolute;
  z-index: 1000;
}
/* Horizontal */
.slide-panes.horizontal > .slide-handler {
  left: 50%;
  bottom: -4px;
  cursor: pointer;
  width: 50px;
  height: 24px;
  margin-left: -26px;
  position: fixed;
  top: auto;
  background: transparent url(../../images/student-central/portfolio/teacherResponse/gripper-up.svg) 50% 50% no-repeat;
}
.slide-panes.horizontal > .slide-handler:hover {
  background: transparent url(../../images/student-central/portfolio/teacherResponse/gripper-hover.svg) 50% 50% no-repeat;
}
.slide-panes.horizontal > .slide-handler.slide-panes-dragging {
  background: transparent url(../../images/student-central/portfolio/teacherResponse/gripper-down.svg) 50% 50% no-repeat;
}
.slide-panes.horizontal > .slide-pane1,
.slide-panes.horizontal > .slide-pane2 {
  position: absolute;
  z-index: 10;
  height: calc(100vh - 210px);
}
@media only screen and (min-device-width: 768px) and (max-device-width: 1024px) {
  .slide-panes.horizontal > .slide-pane1,
  .slide-panes.horizontal > .slide-pane2 {
    height: calc(100vh - 268px);
  }
}
.slide-panes.horizontal > .slide-pane1 {
  width: 50%;
  max-width: 720px;
  overflow: hidden;
}
.slide-panes.horizontal > .slide-pane2 {
  left: 50%;
  right: 0;
  overflow-x: auto;
}
